  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "9.3.1",
    "cssnano": "4.1.7",
    "postcss": "7.0.6",
    "postcss-calc": "7.0.1",
    "postcss-cli": "6.0.1",
    "postcss-color-function": "4.0.1",
    "postcss-custom-media": "7.0.7",
    "postcss-custom-properties": "8.0.9",
    "postcss-discard-comments": "4.0.1",
    "postcss-import": "12.0.1",
    "postcss-importantly": "0.0.7",
